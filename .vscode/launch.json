{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron: Run App",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "electron"
      ],
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}